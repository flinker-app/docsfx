{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "404.html"
        ]
      }
    ],
    "exclude": [
      "_site/**"
    ],
    "resource": [
      {
        "files": [
          "_media/**",
          "favicon.ico"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "my-template"
    ],
    "styles": [
      "my-template/public/main.css"
    ]
  },
  "sitemap": {
    "baseUrl": "https://docs.flinker.app"
  },
  "globalMetadata": {
    "_appName": "",
    "_appTitle": "docs",
    "_enableNewTab": true,
    "_appLogoPath": "/_media/flinker-icon.png",
    "_appFaviconPath": "favicon.ico",
    "_appfooter": "<div id=\\\"footer\\\" style=\\\" flex-direction: column;display: flex;\\\"><span style=\\\"margin-top: 16px;\\\">Flinker Docs with ðŸ’™</span></div>",
    "_enableSearch": true,
    "_disableContribution": true,
    "pdf": false
  }
}
