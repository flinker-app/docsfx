{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
          
        ],
        "exclude": [
          "_site/**"
        ]
      },
    ],
    "resource": [
      {
        "files": [
          "_media/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "my-template"
    ],
    "globalMetadata": {
      "_appLogoPath": "",
      "_appName": "Home",
      "_appTitle": "docs",
      "_enableNewTab": true,
      "_appFaviconPath": "https://cdn.prod.website-files.com/62c5bbce6bd5588d4aeb6a81/62c5bbce6bd5588304eb6c61_flinker%20fav%20icon.png",
